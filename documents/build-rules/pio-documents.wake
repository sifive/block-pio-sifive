
# Notify others we are providing documentation.
def project = simplify "{here}/.."
publish docDirectory =  project, Nil

# Build the pio document onlining document from scratch
global def makePioDocument Unit = makeOnboardDocument pioDUT


# Build a document using the test pio configuration.
global def testPioDocument Unit   =
    buildOnboardDocument "PioTest" (source "{project}/test_config/objectModel.json") "{project}/test_config/build"
